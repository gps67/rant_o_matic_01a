[ -d tmp ] || mkdir tmp
. fns_var_add
var_pfx PATH /home/gps/prefix/bin ':'
(
	gnuplot --version
	plot_daily_volume.awk "$@" 
	show_as_dates.tcl
	plot_min_max_range "$@" 
) 2>&1 | tee tmp/tmp_$(basename $0).tmp \
# | less
# )| less

